export(jm, traceplot, gelman_diag, densityplot, cumuplot, value, slope, area,
       coef, fixef, ranef, terms, model.frame, model.matrix, family)

import(Rcpp)
importFrom("stats", "coef", "terms", "model.frame", "model.matrix", "family")
importFrom("parallel", "makeCluster", "stopCluster", "detectCores", "parLapply")
importFrom("nlme", "fixef", "ranef")
importFrom("coda", "as.mcmc.list", "as.mcmc", "traceplot")


useDynLib(JMbayes2, .registration = TRUE)

S3method(traceplot, jm)
S3method(gelman_diag, jm)
S3method(densityplot, jm)
S3method(cumuplot, jm)

S3method(print, jm)
S3method(summary, jm)
S3method(print, summary.jm)
S3method(fixef, jm)
S3method(ranef, jm)
S3method(terms, jm)
S3method(model.frame, jm)
S3method(model.matrix, jm)
S3method(family, jm)

